{
  "name": "payload-test",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation pnpm run generate:prisma && next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "generate:prisma": "prisma generate --schema prisma/schema.prisma",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start"
  },
  "dependencies": {
    "@better-fetch/fetch": "1.1.12",
    "@hookform/resolvers": "3.9.1",
    "@payload-enchants/better-localized-fields": "^1.2.2",
    "@payload-enchants/docs-reorder": "^1.2.2",
    "@payloadcms/db-mongodb": "^3.5.0",
    "@payloadcms/db-sqlite": "^3.5.0",
    "@payloadcms/email-nodemailer": "^3.5.0",
    "@payloadcms/live-preview": "^3.5.0",
    "@payloadcms/live-preview-react": "^3.5.0",
    "@payloadcms/next": "^3.5.0",
    "@payloadcms/payload-cloud": "^3.5.0",
    "@payloadcms/plugin-cloud-storage": "^3.5.0",
    "@payloadcms/richtext-lexical": "^3.5.0",
    "@payloadcms/storage-s3": "^3.5.0",
    "@payloadcms/translations": "^3.5.0",
    "@payloadcms/ui": "3.5.0",
    "@prisma/client": "6.0.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@types/nodemailer": "^6.4.17",
    "better-auth": "^1.0.10",
    "class-variance-authority": "^0.7.1",
    "classnames": "^2.5.1",
    "cloudinary": "^2.5.1",
    "cross-env": "^7.0.3",
    "graphql": "^16.9.0",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.462.0",
    "next": "15.0.4",
    "next-intl": "^3.26.0",
    "next-nprogress-bar": "^2.3.15",
    "nodemailer": "^6.9.16",
    "nodemailer-postmark-transport": "^6.0.0",
    "payload": "^3.5.0",
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.0",
    "postmark": "^4.0.5",
    "prisma": "^6.0.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.53.2",
    "sharp": "0.32.6",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.10.1",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.0.0",
    "typescript": "5.6.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  }
}
