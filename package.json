{
  "name": "payload-test",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation pnpm run generate:prisma && next build",
    "dev": "pnpm generate:prisma && pnpm generate:importmap && pnpm generate:types && pnpm codegen  & cross-env NODE_OPTIONS=--no-deprecation next dev --turbo",
    "dev:local": "cross-env NODE_OPTIONS=--no-deprecation DATABASE_URI=\"mongodb://localhost:27017/payload?retryWrites=true&w=majority\" next dev & pnpm codegen",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "generate:prisma": "prisma generate --schema auth/prisma/schema.prisma",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "codegen": "graphql-codegen --watch",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "mongo:start": "brew services start mongodb-community",
    "mongo:stop": "brew services stop mongodb-community",
    "reset": "rm -rf ./node_modules && rm -rf ./.next && pnpm i && pnpm generate:prisma && && pnpm generate:types && pnpm generate:importmap "
  },
  "dependencies": {
    "@better-fetch/fetch": "1.1.12",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
